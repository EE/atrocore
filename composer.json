{
  "name": "atrocore/core",
  "description": "AtroCORE. Open source application.",
  "homepage": "https://atrocore.com",
  "license": "GPL-3.0-only",
  "minimum-stability": "stable",
  "require": {
    "php": "^7.4",
    "ext-pdo_mysql": "*",
    "ext-openssl": "*",
    "ext-json": "*",
    "ext-zip": "*",
    "ext-gd": "*",
    "ext-mbstring": "*",
    "ext-xml": "*",
    "ext-curl": "*",
    "ext-exif": "*",
    "ext-imagick": "*",
    "gumlet/php-image-resize": "~1.9.2",
    "composer/semver": "~1.4.0",
    "doctrine/dbal": "~2.4.2",
    "slim/slim": "~2.6.2",
    "mtdowling/cron-expression": "~1.0.3",
    "zendframework/zend-mail": "~2.10.0",
    "zendframework/zend-servicemanager": "~3.3.2",
    "monolog/monolog": "~1.20.0",
    "yzalis/identicon": "~1.1.0",
    "zordius/lightncandy": "~0.21",
    "symfony/event-dispatcher": "~4.3.0",
    "symfony/workflow": "~4.3.0"
